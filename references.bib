  keywords = {cytomegalovirus,EMT,GSEA,MET,RNA-seq},
  file = {/Users/mcgaugheyd/Zotero/storage/MRTZYQ84/Oberstein and Shenk - 2017 - Cellular responses to human cytomegalovirus infect.pdf;/Users/mcgaugheyd/Zotero/storage/NUQ93FV2/E8244.html}
}

