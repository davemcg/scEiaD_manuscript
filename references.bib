}

@article{hawkinsDistinctEpigenomicLandscapes2010,
  title = {Distinct {{Epigenomic Landscapes}} of {{Pluripotent}} and {{Lineage}}-{{Committed Human Cells}}},
  author = {Hawkins, R. David and Hon, Gary C. and Lee, Leonard K. and Ngo, QueMinh and Lister, Ryan and Pelizzola, Mattia and Edsall, Lee E. and Kuan, Samantha and Luu, Ying and Klugman, Sarit and Antosiewicz-Bourget, Jessica and Ye, Zhen and Espinoza, Celso and Agarwahl, Saurabh and Shen, Li and Ruotti, Victor and Wang, Wei and Stewart, Ron and Thomson, James A. and Ecker, Joseph R. and Ren, Bing},
