  pmcid = {PMC6521882},
  keywords = {Adaptor Proteins; Signal Transducing,Animals,Cell Cycle Proteins,Cell Proliferation,Cellular Reprogramming,Cyclin D1,Cyclin D3,Ependymoglial Cells,Hippo pathway,LATS,Mammals,Mice,Müller glia,Neuroglia,Protein-Serine-Threonine Kinases,regeneration,reprogramming,retina,Retina,Signal Transduction,Stem Cells,YAP},
  file = {/Users/mcgaugheyd/Zotero/storage/Y7MRM3U8/Rueda et al. - 2019 - The Hippo Pathway Blocks Mammalian Retinal Müller .pdf}
