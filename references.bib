@article{havrillaMapConstrainedCoding2017,
  title = {A Map of Constrained Coding Regions in the Human Genome.},
  author = {Havrilla, James M. and Pedersen, Brent S. and Layer, Ryan M. and Quinlan, Aaron R.},
