  file = {/Users/mcgaugheyd/Zotero/storage/BTEENT7I/Beyan et al. - 2012 - Guthrie card methylomics identifies temporally sta.pdf;/Users/mcgaugheyd/Zotero/storage/8JNQGT7B/gr.134304.111.html}
}

@article{bhartiOtherPigmentCell2006,
  title = {The Other Pigment Cell: Specification and Development of the Pigmented Epithelium of the Vertebrate Eye},
