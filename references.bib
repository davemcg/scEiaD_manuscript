@article{simonINSCTIntegratingMillions2020,
  title = {{{INSCT}}: {{Integrating}} Millions of Single Cells Using Batch-Aware Triplet Neural Networks},
  shorttitle = {{{INSCT}}},
  author = {Simon, Lukas M. and Wang, Yin-Ying and Zhao, Zhongming},
  date = {2020-05-17},
  journaltitle = {bioRxiv},
  pages = {2020.05.16.100024},
  publisher = {{Cold Spring Harbor Laboratory}},
