@article{voigtSingleCellRNASequencing2020,
  title = {Single-{{Cell RNA Sequencing}} in {{Human Retinal Degeneration Reveals Distinct Glial Cell Populations}}},
  author = {Voigt, Andrew P. and Binkley, Elaine and Flamme-Wiese, Miles J. and Zeng, Shemin and DeLuca, Adam P. and Scheetz, Todd E. and Tucker, Budd A. and Mullins, Robert F. and Stone, Edwin M.},
  date = {2020-02-13},
