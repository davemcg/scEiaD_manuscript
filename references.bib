@article{aomatsuNovelSCRG1BST12014,
  title = {Novel {{SCRG1}}/{{BST1}} Axis Regulates Self-Renewal, Migration and Osteogenic Differentiation Potential in Mesenchymal Stem Cells},
  author = {Aomatsu, Emiko and Takahashi, Noriko and Sawada, Shunsuke and Okubo, Naoto and Hasegawa, Tomokazu and Taira, Masayuki and Miura, Hiroyuki and Ishisaki, Akira and Chosa, Naoyuki},
  date = {2014-01-13},
