@article{soriano-tarragaDNAIsolationMethod2013,
  title = {{{DNA Isolation Method Is}} a {{Source}} of {{Global DNA Methylation Variability Measured}} with {{LUMA}}. {{Experimental Analysis}} and a {{Systematic Review}}},
  author = {Soriano-Tárraga, Carolina and Jiménez-Conde, Jordi and Giralt-Steinhauer, Eva and Ois, Angel and Rodríguez-Campello, Ana and Cuadrado-Godia, Elisa and Fernández-Cadenas, Israel and Montaner, Joan and Lucas, Gavin and Elosua, Roberto and Roquer, Jaume},
  date = {2013},
  journaltitle = {PloS one},
  shortjournal = {PLoS ONE},
  volume = {8},
  number = {4},
  eprint = {23585847},
  eprinttype = {pmid},
  pages = {e60750},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0060750},
  abstract = {In DNA methylation, methyl groups are covalently bound to CpG dinucleotides. However, the assumption that methyl groups are not lost during routine DNA extraction has not been empirically tested. To avoid nonbiological associations in DNA methylation studies, it is essential to account for potential batch effect bias in the assessment of this epigenetic mechanism. Our purpose was to determine if the DNA isolation method is an independent source of variability in methylation status. We quantified Global DNA Methylation (GDM) by luminometric methylation assay (LUMA), comparing the results from 3 different DNA isolation methods. In the controlled analysis (n{$\mkern1mu$}={$\mkern1mu$}9), GDM differed slightly for the same individual depending on extraction method. In the population analysis (n{$\mkern1mu$}={$\mkern1mu$}580) there were significant differences in GDM between the 3 DNA isolation methods (medians, 78.1\%, 76.5\% and 75.1\%; p{$<$}0.001). A systematic review of published data from LUMA GDM studies that specify DNA extraction methods is concordant with our findings. DNA isolation method is a source of GDM variability measured with LUMA. To avoid possible bias, the method used should be reported and taken into account in future DNA methylation studies.},
  langid = {english},
  keywords = {DNA Methylation,luma,Method}
}

@article{sridharSingleCellTranscriptomicComparison2020,
  title = {Single-{{Cell Transcriptomic Comparison}} of {{Human Fetal Retina}}, {{hPSC}}-{{Derived Retinal Organoids}}, and {{Long}}-{{Term Retinal Cultures}}},
  author = {Sridhar, Akshayalakshmi and Hoshino, Akina and Finkbeiner, Connor R. and Chitsazan, Alex and Dai, Li and Haugan, Alexandra K. and Eschenbacher, Kayla M. and Jackson, Dana L. and Trapnell, Cole and Bermingham-McDonogh, Olivia and Glass, Ian and Reh, Thomas A.},
  date = {2020-02-04},
  journaltitle = {Cell Reports},
  shortjournal = {Cell Rep},
  volume = {30},
  number = {5},
  eprint = {32023475},
  eprinttype = {pmid},
  pages = {1644-1659.e4},
  issn = {2211-1247},
  doi = {10.1016/j.celrep.2020.01.007},
  abstract = {To study the development of the human retina, we use single-cell RNA sequencing (RNA-seq) at key fetal stages and follow the development of the major cell types as well as populations of transitional cells. We also analyze stem cell (hPSC)-derived retinal organoids; although organoids have a very similar cellular composition at equivalent ages as the fetal retina, there are some differences in gene expression of particular cell types. Moreover, the inner retinal lamination is disrupted at more advanced stages of organoids compared with fetal retina. To determine whether the disorganization in the inner retina is due to the culture conditions, we analyze retinal development in fetal retina maintained under similar conditions. These retinospheres develop for~at least 6~months, displaying better inner retinal lamination~than retinal organoids. Our single-cell RNA sequencing (scRNA-seq) comparisons of fetal retina, retinal organoids, and retinospheres provide a resource for developing better in~vitro models for retinal disease.},
  langid = {english},
  keywords = {lamination,neurogenesis,organoids,stem cells}
}

