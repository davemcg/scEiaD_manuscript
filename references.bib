@article{wagnerTransientReceptorPotential2008,
  title = {Transient Receptor Potential {{M3}} Channels Are Ionotropic Steroid Receptors in Pancreatic Beta Cells},
  author = {Wagner, Thomas F. J. and Loch, Sabine and Lambert, Sachar and Straub, Isabelle and Mannebach, Stefanie and Mathar, Ilka and Düfer, Martina and Lis, Annette and Flockerzi, Veit and Philipp, Stephan E. and Oberwinkler, Johannes},
