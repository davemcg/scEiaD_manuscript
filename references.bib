@article{nozawaSAFARegulatesInterphase2017,
  title = {{{SAF}}-{{A Regulates Interphase Chromosome Structure}} through {{Oligomerization}} with {{Chromatin}}-{{Associated RNAs}}},
  author = {Nozawa, Ryu-Suke and Boteva, Lora and Soares, Dinesh C. and Naughton, Catherine and Dun, Alison R. and Buckle, Adam and Ramsahoye, Bernard and Bruton, Peter C. and Saleeb, Rebecca S. and Arnedo, Maria and Hill, Bill and Duncan, Rory R. and Maciver, Sutherland K. and Gilbert, Nick},
  date = {2017-06-15},
  journaltitle = {Cell},
  shortjournal = {Cell},
  volume = {169},
  number = {7},
  pages = {1214-1227.e18},
  issn = {0092-8674},
  doi = {10.1016/j.cell.2017.05.029},
  url = {http://www.sciencedirect.com/science/article/pii/S0092867417305901},
  urldate = {2019-01-17},
  abstract = {Summary Higher eukaryotic chromosomes are organized into topologically constrained functional domains; however, the molecular mechanisms required to sustain these complex interphase chromatin structures are unknown. A stable matrix underpinning nuclear organization was hypothesized, but the idea was abandoned as more dynamic models of chromatin behavior became prevalent. Here, we report that scaffold attachment factor A (SAF-A), originally identified as a structural nuclear protein, interacts with chromatin-associated RNAs (caRNAs) via its RGG domain to regulate human interphase chromatin structures in a transcription-dependent manner. Mechanistically, this is dependent on SAF-A’s AAA+ ATPase domain, which mediates cycles of protein oligomerization with caRNAs, in response to ATP binding and hydrolysis. SAF-A oligomerization decompacts large-scale chromatin structure while SAF-A loss or monomerization promotes aberrant chromosome folding and accumulation of genome damage. Our results show that SAF-A and caRNAs form a dynamic, transcriptionally responsive chromatin mesh that organizes large-scale chromosome structures and protects the genome from instability.},
  keywords = {AAA ATPases,chromatin,chromatin compaction,chromatin-associated RNAs,chromosome stability,hnRNPU,nuclear architecture,SAF-A,transcription}
}

