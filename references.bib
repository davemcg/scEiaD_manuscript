@article{chenTissuesCellTypes2018,
  title = {From {{Tissues}} to {{Cell Types}} and {{Back}}: {{Single}}-{{Cell Gene Expression Analysis}} of {{Tissue Architecture}}},
  shorttitle = {From {{Tissues}} to {{Cell Types}} and {{Back}}},
  author = {Chen, Xi and Teichmann, Sarah A. and Meyer, Kerstin B.},
  date = {2018-07-20},
  journaltitle = {Annual Review of Biomedical Data Science},
  shortjournal = {Annu. Rev. Biomed. Data Sci.},
  volume = {1},
  number = {1},
  pages = {29--51},
  issn = {2574-3414},
  doi = {10.1146/annurev-biodatasci-080917-013452},
  url = {https://www.annualreviews.org/doi/10.1146/annurev-biodatasci-080917-013452},
  urldate = {2019-07-27},
  abstract = {With the recent transformative developments in single-cell genomics and, in particular, single-cell gene expression analysis, it is now possible to study tissues at the single-cell level, rather than having to rely on data from bulk measurements. Here we review the rapid developments in single-cell RNA sequencing (scRNA-seq) protocols that have the potential for unbiased identification and profiling of all cell types within a tissue or organism. In addition, novel approaches for spatial profiling of gene expression allow us to map individual cells and cell types back into the three-dimensional context of organs. The combination of in-depth single-cell and spatial gene expression data will reveal tissue architecture in unprecedented detail, generating a wealth of biological knowledge and a better understanding of many diseases.},
  file = {/Users/mcgaugheyd/Zotero/storage/NJBSR474/Chen et al. - 2018 - From Tissues to Cell Types and Back Single-Cell G.pdf;/Users/mcgaugheyd/Zotero/storage/9Z9STYZ4/annurev-biodatasci-080917-013452.html}
}

