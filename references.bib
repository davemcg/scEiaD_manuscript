}

@article{lehmannSinglecellProfilingReveals2020,
  title = {Single-Cell Profiling Reveals an Endothelium-Mediated Immunomodulatory Pathway in the Eye Choroid},
  author = {Lehmann, Guillermo L. and Hanke-Gogokhia, Christin and Hu, Yang and Bareja, Rohan and Salfati, Zelda and Ginsberg, Michael and Nolan, Daniel J. and Mendez-Huergo, Santiago P. and Dalotto-Moreno, Tomas and Wojcinski, Alexandre and Ochoa, Francisca and Zeng, Shemin and Cerliani, Juan P. and Panagis, Lampros and Zager, Patrick J. and Mullins, Robert F. and Ogura, Shuntaro and Lutty, Gerard A. and Bang, Jakyung and Zippin, Jonathan H. and Romano, Carmelo and Rabinovich, Gabriel A. and Elemento, Olivier and Joyner, Alexandra L. and Rafii, Shahin and Rodriguez-Boulan, Enrique and Benedicto, Ignacio},
  date = {2020-03-20},
