@Article{tranSingleCellProfilesRetinal2019,
  title = {Single-{{Cell Profiles}} of {{Retinal Ganglion Cells Differing}} in {{Resilience}} to {{Injury Reveal Neuroprotective Genes}}},
  author = {Nicholas M. Tran and Karthik Shekhar and Irene E. Whitney and Anne Jacobi and Inbal Benhar and Guosong Hong and Wenjun Yan and Xian Adiconis and McKinzie E. Arnold and Jung Min Lee and Joshua Z. Levin and Dingchang Lin and Chen Wang and Charles M. Lieber and Aviv Regev and Zhigang He and Joshua R. Sanes},
