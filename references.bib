  title = {Assessing the Pathogenicity, Penetrance and Expressivity of Putative Disease-Causing Variants in a Population Setting},
  author = {Wright, Caroline F. and West, Ben and Tuke, Marcus and Jones, Samuel E. and Patel, Kashyap and Laver, Thomas W. and Beaumont, Robin N. and Tyrrell, Jessica and Wood, Andrew R. and Frayling, Timothy M. and Hattersley, Andrew T. and Weedon, Michael N.},
