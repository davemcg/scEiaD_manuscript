@article{clarkComprehensiveAnalysisRetinal2018,
  title = {Comprehensive Analysis of Retinal Development at Single Cell Resolution Identifies {{NFI}} Factors as Essential for Mitotic Exit and Specification of Late-Born Cells},
  author = {Clark, Brian and Stein-O'Brien, Genevieve and Shiau, Fion and Cannon, Gabrielle and Davis, Emily and Sherman, Thomas and Rajaii, Fatemeh and James-Esposito, Rebecca and Gronostajski, Richard and Fertig, Elana and Goff, Loyal and Blackshaw, Seth},
  date = {2018-07-30},
  journaltitle = {bioRxiv},
  pages = {378950},
  doi = {10.1101/378950},
  url = {https://www.biorxiv.org/content/early/2018/07/30/378950},
  urldate = {2019-01-18},
  abstract = {Precise temporal control of gene expression in neuronal progenitors is necessary for correct regulation of neurogenesis and cell fate specification. However, the extensive cellular heterogeneity of the developing CNS has posed a major obstacle to identifying the gene regulatory networks that control these processes. To address this, we used single cell RNA-sequencing to profile ten developmental stages encompassing the full course of retinal neurogenesis. This allowed us to comprehensively characterize changes in gene expression that occur during initiation of neurogenesis, changes in developmental competence, and specification and differentiation of each of the major retinal cell types. These data identify transitions in gene expression between early and late-stage retinal progenitors, as well as a classification of neurogenic progenitors. We identify here the NFI family of transcription factors (Nfia, Nfib, and Nfix) as genes with enriched expression within late RPCs, and show they are regulators of bipolar interneuron and Muller glia specification and the control of proliferative quiescence.},
  langid = {english},
  file = {/Users/mcgaugheyd/Zotero/storage/AYW886ZD/Clark et al. - 2018 - Comprehensive analysis of retinal development at s.pdf;/Users/mcgaugheyd/Zotero/storage/8R8UQMSH/378950.html}
}

