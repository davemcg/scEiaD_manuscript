@article{roselRNASeqAnalysisMutant2011,
  title = {{{RNA}}-{{Seq}} Analysis in Mutant Zebrafish Reveals Role of {{U1C}} Protein in Alternative Splicing Regulation},
  author = {Rösel, Tanja Dorothe and Hung, Lee-Hsueh and Medenbach, Jan and Donde, Katrin and Starke, Stefan and Benes, Vladimir and Rätsch, Gunnar and Bindereif, Albrecht},
  date = {2011-05-18},
